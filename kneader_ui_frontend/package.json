{
  "name": "kneader-ui",
  "version": "1.0.0",
  "main": "electron-main.js",
  "description": "UI for Kneader Controller System",

  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "electron .",
    "dist": "electron-builder"
  },

  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },

  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "electron": "^40.1.0",
    "electron-builder": "^24.13.3"
  },

  "build": {
    "appId": "com.kneader.app",
    "productName": "Kneader",
    "win": {
      "target": "nsis"
    },
    "protocols": [
      {
        "name": "Kneader Protocol",
        "schemes": ["kneader"]
      }
    ]
  }
}
